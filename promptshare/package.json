{
  "name": "basic",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production && npm run build:css",
    "preview": "wrangler pages dev",
    "deploy": "$npm_execpath run build && wrangler pages deploy",
    "start": "vite preview --port 8788",
    "build:css": "tailwindcss -i ./app/styles.css -o ./dist/styles.css"
  },
  "private": true,
  "dependencies": {
    "hono": "^4.6.3",
    "honox": "^0.1.24"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240529.0",
    "@hono/vite-build": "^1.0.0",
    "@hono/vite-dev-server": "^0.15.1",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "daisyui": "^4.12.10",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.5.3",
    "vite": "^5.2.12",
    "vite-plugin-compression": "^0.5.1",
    "wrangler": "^3.57.2"
  }
}
